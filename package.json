{
  "name": "bp-esa-backend",
  "version": "1.0.0",
  "description": "Engagement Support Automation ",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production tsc && node build/bin/Server.js",
    "start:dev": "cross-env NODE_ENV=development node scripts/start.js",
    "clean": "rimraf coverage build",
    "debug": "NODE_ENV=production npm run build && node --inspect-brk=0.0.0.0:9229 ./build/index.js",
    "start:debug": "NODE_ENV=development nodemon ./server/index --exec babel-node  --inspect-brk=0.0.0.0:9229 --watch ./server",
    "build": "babel ./server -d ./build",
    "pretest": "NODE_ENV=test npm run reset-database && NODE_ENV=test npm run migrations",
    "test": "NODE_ENV=test nyc --reporter=lcov --reporter=html --reporter=text mocha --recursive test/* --compilers js:@babel/register --require @babel/polyfill --timeout 150000 --exit",
    "lint": "eslint .",
    "migrations": "sequelize db:migrate",
    "reset-database": "npx redis-cli FLUSHALL && sequelize db:drop && sequelize db:create",
    "docs": "./node_modules/.bin/jsdoc -c jsdoc.json",
    "delete-partners": "babel-node ./server/deletePartners.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bopchy/bp-esa-backend.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Bopchy/bp-esa-backend/issues"
  },
  "homepage": "https://github.com/Bopchy/bp-esa-backend#readme",
  "dependencies": {
    "@babel/node": "^7.0.0",
    "@slack/client": "^5.0.0",
    "axios": "^0.19.0",
    "body-parser": "^1.18.3",
    "chai-http": "^4.2.1",
    "chalk": "^2.4.2",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "faker": "^4.1.0",
    "googleapis": "^33.0.0",
    "http": "0.0.0",
    "http-proxy": "^1.17.0",
    "jsdoc": "^3.6.2",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "ms": "^2.1.1",
    "nodemailer": "^4.6.8",
    "pg": "^7.4.3",
    "pg-hstore": "^2.3.2",
    "proxyquire": "^2.1.0",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^4.38.0",
    "sequelize-cli": "^4.0.0",
    "sequelize-replace-enum-postgres": "^1.5.0",
    "socket.io": "^2.2.0",
    "ts-node-dev": "^1.0.0-pre.40",
    "tslib": "^1.10.0",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "^7.4.4",
    "@types/cors": "^2.8.5",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/jest": "^24.0.12",
    "@types/ms": "^0.7.30",
    "@types/node": "^8.0.29",
    "@types/socket.io": "^2.1.2",
    "awesome-typescript-loader": "^5.2.1",
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.13.0",
    "expect": "^23.5.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.5",
    "nyc": "^13.1.0",
    "prettier-eslint": "^8.8.2",
    "rimraf": "^2.6.3",
    "sequelize-test-helpers": "^1.0.3",
    "sinon": "^6.3.5",
    "sinon-chai": "^3.2.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.3"
  }
}
